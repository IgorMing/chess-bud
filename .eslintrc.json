{
  "root": true,
  "extends": ["@react-native-community", "prettier", "plugin:react-hooks/recommended"],
  "plugins": ["import", "react-hooks"],
  "rules": {
    "import/no-unresolved": "error",
    "react-native/no-inline-styles": "off",
    "prettier/prettier": "off"
  },
  "settings": {
    "import/parsers": {
      "@typescript-eslint/parser": [".ts", ".tsx"]
    },
    "import/resolver": {
      "typescript": {
        "alwaysTryTypes": true
      }
    }
  }
}
