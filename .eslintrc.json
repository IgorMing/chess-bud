{
  "root": true,
  "extends": "@react-native-community",
  "plugins": ["import"],
  "rules": {
    "import/no-unresolved": "error",
    "react-native/no-inline-styles": "off" // remove this later
  },
  "settings": {
    "import/parsers": {
      "@typescript-eslint/parser": [".ts", ".tsx"]
    },
    "import/resolver": {
      "typescript": {
        "alwaysTryTypes": true
      }
    }
  }
}
